{
  "expo": {
    "name": "FitHealthSync",
    "slug": "FitHealthSync",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "scheme": "fithealthsyncredirect",
    "splash": {
      "image": "./app/assets/fit-background.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lefthandmagic.FitHealthSync",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "location"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ],
      "package": "com.lefthandmagic.FitHealthSync"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "",
    "plugins": [
      "react-native-health",
      "expo-task-manager",
      "expo-background-fetch"
    ]
  }
}
